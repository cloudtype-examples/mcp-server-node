    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.0.0",
    "node-fetch": "^3.3.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
